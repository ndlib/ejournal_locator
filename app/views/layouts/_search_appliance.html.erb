<div id="tab_box" class="search-appliance">
  <dl class="tabs contained">
    <dd><a tabindex="1" href="http://<%= Rails.configuration.library_host %>/search/onesearch" id="tab_primo_l" class="onesearch tab"><span>OneSearch</span></a></dd>
    <dd><a tabindex="2" href="http://<%= Rails.configuration.library_host %>/search/catalog" id="tab_cat_l" class="ndcatalog tab"><span>ND Catalog</span></a></dd>
    <dd><a tabindex="3" href="http://<%= Rails.configuration.library_host %>/search/articles" id="tab_art_l" class="tab">Articles</a></li>
    <dd><a tabindex="4" href="#tab_jou" id="tab_jou_l" class="active tab">eJournals</a></dd>
    <dd><a tabindex="5" href="http://<%= Rails.configuration.library_host %>/search/databases" id="tab_dat_l" class="tab">Databases</a></dd>
    <dd><a tabindex="6" href="http://<%= Rails.configuration.library_host %>/search/ebooks" id="tab_boo_l" class="tab">eBooks</a></dd>
  </dl>

  <div id="search_box">
    <ul class="tabs-content contained">
      <li class="active tab_jou" id="tab_jouTab">
        <%= render_search_bar %>

        <ul class="extra-links">
          <li><a href="http://library.nd.edu/eresources/ejournals/alljournals">All journals</a></li>
        </ul>
      </li>
    </ul>
  </div>
</div>
